{
  "name": "three-ifc",
  "version": "0.2.0",
  "description": "Lightweight wrapper around web-ifc",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./graphics": {
      "import": "./dist/graphics/index.mjs",
      "require": "./dist/graphics/index.js",
      "types": "./dist/graphics/index.d.ts"
    },
    "./graphics/events": {
      "import": "./dist/graphics/events/index.mjs",
      "require": "./dist/graphics/events/index.js",
      "types": "./dist/graphics/events/index.d.ts"
    },
    "./utils/flatbuffersConverters": {
      "import": "./dist/utils/flatbuffersConverters/index.mjs",
      "require": "./dist/utils/flatbuffersConverters/index.js",
      "types": "./dist/utils/flatbuffersConverters/index.d.ts"
    }
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsup",
    "test": "vitest"
  },
  "author": "contact@jonasfrei.de",
  "license": "MIT",
  "dependencies": {
    "flatbuffers": "^25.9.23",
    "three": "^0.181.2",
    "uuid": "^13.0.0",
    "web-ifc": "^0.0.74"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/three": "^0.181.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
